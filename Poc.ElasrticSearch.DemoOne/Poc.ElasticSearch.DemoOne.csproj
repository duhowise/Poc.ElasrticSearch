    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>63342f1b-bf54-43b9-9e2f-f85b859e064c</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
